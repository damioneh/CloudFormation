{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "",
  "Metadata": {},
  "Parameters": {
    "EnvName": {
      "Description": "Choose environment:",
      "Type": "String",
      "AllowedValues": ["Dev", "Stg"],
      "ConstraintDescription": "Environment must match with specified AllowedValue"
    },

    "VpcCIDR": {
      "Description": "Create a CIDR block for W-F VPC",
      "Type": "String",
      "Default": "10.0.0.0/16"
    },

    "PubSubCIDR": {
      "Description": "Create a CIDR block for W-F PubSub",
      "Type": "String",
      "Default": "10.0.1.0/28"
    },

    "PrvSubCIDR": {
      "Description": "Create a CIDR block for W-F PrvSub",
      "Type": "String",
      "Default": "10.0.1.20/28"
    },

    "wfKeyPair": {
      "Description": "w-f KeyPair for EC2 Instance",
      "Type": "String",
      "Default": "JJTech-KeyPair-01"
    },

    "wfImageId": {
      "Description": "NOVA Region Image Id",
      "Type": "String",
      "Default": "ami-0022f774911c1d690"
    },

    "wfInstanceType": {
      "Description": "W-F EC2 Instance Type",
      "Type": "String",
      "Default": "t2.micro"
    }
  },
  "Mappings": {
    "AWSInstanceType2Arch": {
      "t1.micro": { "Arch": "HVM64" },
      "t2.nano": { "Arch": "HVM64" },
      "t2.micro": { "Arch": "HVM64" },
      "t2.small": { "Arch": "HVM64" },
      "t2.medium": { "Arch": "HVM64" }
    },
    "AWSRegionArch2AMI": {
      "us-east-1": {
        "HVM64": "ami-032930428bf1abbff",
        "HVMG2": "ami-0aeb704d503081ea6"
      },
      "us-east-2": {
        "HVM64": "ami-027cab9a7bf0155df",
        "HVMG2": "NOT_SUPPORTED"
      },
      "us-west-1": {
        "HVM64": "ami-088c153f74339f34c",
        "HVMG2": "ami-0a7fc72dc0e51aa77"
      },
      "us-west-2": {
        "HVM64": "ami-01fee56b22f308154",
        "HVMG2": "ami-0fe84a5b4563d8f27"
      }
    }
  },

  "Conditions": {},
  "Resources": {},
  "Outputs": {}
